#!/bin/bash
address=$(cat address)
server=$(ioctl config get endpoint | cut -f1 -d " ")

numActions=$(./getNumActions)
lastActionIdx=$(($numActions - 1))
if [ $lastActionIdx == -1 ]
  then
    exit
  else
	grpcurl -plaintext -d '{"byAddr": {"address": "'$address'", "start": '$lastActionIdx', "count": 1}}' $server iotexapi.APIService.GetActions | jq -r '.actionInfo[0].actHash'
  fi
